{
  "name": "jsbeats",
  "version": "0.0.3",
  "description": "Example Music Player App feat. Sails.js, ES6, Angular (v1.x), Jade, Gulp, Browserify, Babel, and Material Design",
  "main": "app.js",
  "scripts": {
    "debug": "DEBUG=* nodemon app.js",
    "build": "gulp build",
    "watch": "gulp watch",
    "test": "mocha test",
    "start": "nodemon app",
    "docker-build": "docker build -t jsbeats .",
    "docker-run": "docker run --name jsbeats --detach -p 1337:1337 jsbeats",
    "docker-run-dev": "docker run --name jsbeats --rm -p 1337:1337 -v $PWD:/app jsbeats"
  },
  "author": "Dan Levy <Dan@DanLevy.net>",
  "license": "MIT",
